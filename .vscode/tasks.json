    "version": "2.0.0",
    "tasks": [
        {
        "taskName" : "launch gae server",
        "command": "python",
        "type":"shell",
        "presentation" : { "reveal" : "always"},
        "args": [
            "'C:/Program Files (x86)/Google/Cloud SDK/google-cloud-sdk/bin/dev_appserver.py'",
            "--python_startup_script=${workspaceRoot}/pydev_startup.py",
            "--automatic_restart=no",
            "--max_module_instances=default:1",
            "${workspaceRoot}/app.yaml"
            ]
        }
    ]

}